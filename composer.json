{
    "name": "fragotesac/laminas-api-tools-documentation-security",
    "description": "A Laminas package for adding security to the API documentation",
    "type": "library",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0"
    },
    "autoload": {
        "psr-4": {
            "LaminasApiToolsDocumentationSecurity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasApiToolsDocumentationSecurity\\": "test/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Gonzales",
            "email": "francis@fragote.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laminas/laminas-test": "^3.5"
    }
}
